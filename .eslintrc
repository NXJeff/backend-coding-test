{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "amd": true
    },
    "rules": {
        "@typescript-eslint/no-var-requires": 0,
        "camelcase": "on"
    }
}