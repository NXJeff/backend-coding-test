{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha tests",
    "test:coverage": "nyc npm run test",
    "start": "node index.js",
    "lint": "eslint src/**/*.js",
    "format": "eslint src/**/*.js --fix && prettier --write ./src",
    "tsc": "tsc --project ./",
    "prepush": "npm run lint && npm run test:coverage",
    "docs": "jsdoc -c jsdoc.json"
  },
  "pre-push": [
    "test"
  ],
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.0.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "jsdoc": "^3.6.10",
    "mocha": "^6.1.4",
    "supertest": "^4.0.2",
    "eslint": "6.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "prettier": "^2.6.2",
    "nyc": "^15.1.0",
    "chai": "^4.3.6",
    "pre-push": "^0.1.2",
    "typescript": "^4.7.3"
  }
}
