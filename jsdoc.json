{
    "opts": {
        "destination": "./docs/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".(jsx|js|ts|tsx)$",
        "excludePattern": ".+\\.(test|spec).ts"
    }
}